<li id="{{id}}" class="pie-chart tile {{color}}" data-row="{{row}}" data-col="{{col}}" data-sizex="{{sizex}}" data-sizey="{{sizey}}">
	<span class="resize-btn glyphicon glyphicon-resize-full" aria-hidden="true"></span>
	<h4>{{title}}</h4>
	<div class="tile-content-container">
		<div class="pie-chart-legend"></div>
		<div class="chart-container"></div>
	</div>
</li>